{
    "multipart": [
        {   "apply": { "model": "pipez:block/universal_pipe_core" }},
        {   "when": { "north": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "y": 90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "y": 270, "uvlock": false }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "x": 270, "uvlock": false }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "pipez:block/universal_pipe_part", "x": 90, "uvlock": false }
        }
    ]
}